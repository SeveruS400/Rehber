
@if (User.Identity.IsAuthenticated)
{
    <ul class="navbar-nav">
        <li class="nav-item">
            <a class="nav-link text-black" asp-area="" asp-controller="Account" asp-action="Logout">Logout</a>
        </li>
    </ul>
}
else
{
    <ul class="navbar-nav">
        <li class="nav-item">
            <a class="nav-link text-black" asp-area="" asp-controller="Account" asp-action="Login">Login</a>
        </li>
    </ul>
}


